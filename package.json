{
  "scripts": {
    "start": "es-dev-server --node-resolve --watch --http2 --node-resolve --open",
    "start:dist": "es-dev-server --app-index dist/index.html --http2 --node-resolve --open",
    "start:mdl-autocomplete": "es-dev-server --app-index src/mdl-autocomplete/index.html --watch --http2 --node-resolve",
    "site:build": "npm run storybook:build",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o _site -s storybook-static",
    "build": "rimraf dist && rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@open-wc/building-rollup": "^0.6.0",
    "@open-wc/demoing-storybook": "^0.2.0",
    "@pika/web": "^0.4.3",
    "es-dev-server": "^1.6.7",
    "lit-element": "^2.2.0",
    "material-design-lite": "^1.3.0",
    "node-sass": "^4.12.0",
    "node-sass-magic-importer": "^5.3.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.4"
  },
  "dependencies": {
    "lit-html": "^1.1.1"
  }
}
